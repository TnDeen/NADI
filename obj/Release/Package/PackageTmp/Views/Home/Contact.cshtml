            <!-- BEGIN CONTENT -->
            <div class="col-md-12">
                <h1>Contacts</h1>
                <div class="content-page">
                    <div class="row">
                        <div class="col-md-12">
                            <div id="map" class="gmaps margin-bottom-40" style="height:400px;">
                                <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d63752.24744191615!2d101.7265776396571!3d2.9543644491350562!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x31cdcbd32f300b1d%3A0xbc8b1c2b2792e85e!2sBandar+Baru+Bangi%2C+Selangor!5e0!3m2!1sen!2smy!4v1486056798469" width="100%" height="400" frameborder="0" style="border:0" allowfullscreen></iframe>

                            </div>
