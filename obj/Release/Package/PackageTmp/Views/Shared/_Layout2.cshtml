    <!-- Core plugins END (required only for current page) -->
    <!-- Global js BEGIN -->
    <script src="~/Content/assets/frontend/onepage/scripts/layout.js" type="text/javascript"></script>
    <script>
    $(document).ready(function() {
        Layout.init();
    });
    </script>
    <!-- Global js END -->
</body>
