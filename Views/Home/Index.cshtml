@model MVC5.Models.VM.SearchVO
@using MVC5.Common

<!-- Sequence Modern Slider -->
@using (Html.BeginForm("Index", "Listing", null, FormMethod.Post, new { enctype = "multipart/form-data" }))
{
<div id="newsletter-faq" style="margin-top:0px !important;">


    <div class="container">
        <div class="row">

            <div class="col-md-12">
                <p class="lead wow fadeInLeft">Find Property Auctions In Malaysia</p>
            </div>


            <div class="form-group col-md-12">


                <div class="input-group wow fadeInRight">
                    @Html.EditorFor(model => model.Address, new { htmlAttributes = new { @class = "form-control newsletter-input", @placeholder = "Enter Location Name" } })
                    <span class="input-group-btn">
                        <button class="btn btn-inverse btn-hg" type="submit" name="submit">Search</button>
                    </span>
                </div>



            </div>

        </div><!-- /row -->

        <div class="row">




            <div class="form-group col-md-6 wow fadeInRight">

                        @Html.DropDownList("PropertyTypeId", null, htmlAttributes: new { @class = "form-control newsletter-input" })
         

            </div>

            <div class="form-group col-md-3 wow fadeInRight">

                        @Html.DropDownList("NegeriID", null, htmlAttributes: new { @class = "form-control newsletter-input" })
       

            </div>

            <div class="form-group col-md-3 wow fadeInRight">

                        @Html.DropDownList("ListingTypeId", null, htmlAttributes: new { @class = "form-control newsletter-input" })
          



            </div>

        </div><!-- /row -->

        <div class="row">

            <div class="form-group col-md-3 wow fadeInRight">

                @Html.EditorFor(model => model.MinPrice, new { htmlAttributes = new { @class = "form-control newsletter-input", @placeholder = "Min Price (MYR)" } })


            </div>

            <div class="form-group col-md-3 wow fadeInRight">

                @Html.EditorFor(model => model.MaxPrice, new { htmlAttributes = new { @class = "form-control newsletter-input", @placeholder = "Max Price (MYR)" } })


            </div>

            <div class="form-group col-md-3 wow fadeInRight">


                @Html.EditorFor(model => model.MinSize, new { htmlAttributes = new { @class = "form-control newsletter-input", @placeholder = "Min Area (Sq.ft)" } })



            </div>

            <div class="form-group col-md-3 wow fadeInRight">

                @Html.EditorFor(model => model.MaxSize, new { htmlAttributes = new { @class = "form-control newsletter-input", @placeholder = "Max Area (Sq.ft)" } })




            </div>

        </div><!-- /row -->

        <div class="row">

            <div class="form-group col-md-12 wow fadeInRight">


                <button class="btn btn-danger">Go To Advance Search</button>



            </div>

        </div><!-- /row -->


    </div><!-- /container -->


</div>
}

