@model IEnumerable<MVC5.Models.AffiliateComission>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h2>Affiliate</h2>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Introducer.NomborAhli)
