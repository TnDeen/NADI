@model MVC5.Models.VM.TableVO
@using MVC5.Common

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<h2>Upload Data Dari Excel</h2>

<p>
    @Html.ActionLink("Download Template", "DownloadFile")
</p>

@using (Html.BeginForm("Upload", "Transaction", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <div class="form-horizontal">
        <div class="form-group">
            
            <div class="col-md-10">
                <input type="file" name="UploadedFile" class="form-control" />
            </div>
        </div>
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" name="Submit" value="Submit" class="btn btn-default" />
            </div>
        </div>
    </div>
}

<div class="content-page">
    <div class="row">
        <div class="col-md-12" style="display:none;">
            <!-- BEGIN SAMPLE TABLE PORTLET-->
            <div class="portlet box red">
                <div class="portlet-title">
                    <div class="caption">
                        <i class="fa fa-cogs"></i>Registerer Binary
                    </div>
                    <div class="tools">
                        <a href="javascript:;" class="collapse">
                        </a>
                        <a href="#portlet-config" data-toggle="modal" class="config">
                        </a>
                        <a href="javascript:;" class="reload">
                        </a>
                        <a href="javascript:;" class="remove">
                        </a>
                    </div>
                </div>
                <div class="portlet-body">
                    <div class="table-scrollable">
                        <table class="table table-hover">
                            <thead>
                                <tr>

                                    <th>
                                       LEVEL :
                                    </th>
                                    <th>
                                        1 (Direct)
                                    </th>
                                    <th>
                                        2
                                    </th>
                                    <th>
                                        3
                                    </th>
                                    <th>
                                        4
                                    </th>
                                    <th>
                                        5
                                    </th>
                                    <th>
                                        6
                                    </th>
                                    <th>
                                        7
                                    </th>
                                    <th>
                                        8
                                    </th>
                                    <th>
                                        9
                                    </th>
                                    <th>
                                        10
                                    </th>
                                    <th>
                                        11
                                    </th>
                                    <th>
                                        12
                                    </th>
                                </tr>
                            </thead>
                            <tbody>


                                <tr>
                                    <td>Non Active Registerer :</td>
                                    <td>@Model.childNonActiveMap["1"]</td>
                                    <td>@Model.childNonActiveMap["2"]</td>
                                    <td>@Model.childNonActiveMap["3"]</td>
                                    <td>@Model.childNonActiveMap["4"]</td>
                                    <td>@Model.childNonActiveMap["5"]</td>
                                    <td>@Model.childNonActiveMap["6"]</td>
                                    <td>@Model.childNonActiveMap["7"]</td>
                                    <td>@Model.childNonActiveMap["8"]</td>
                                    <td>@Model.childNonActiveMap["9"]</td>
                                    <td>@Model.childNonActiveMap["10"]</td>
                                    <td>@Model.childNonActiveMap["11"]</td>
                                    <td>@Model.childNonActiveMap["12"]</td>
                                </tr>

                                <tr>
                                    <td>Active Registerer :</td>
                                    <td>@Model.childActiveMap["1"]</td>
                                    <td>@Model.childActiveMap["2"]</td>
                                    <td>@Model.childActiveMap["3"]</td>
                                    <td>@Model.childActiveMap["4"]</td>
                                    <td>@Model.childActiveMap["5"]</td>
                                    <td>@Model.childActiveMap["6"]</td>
                                    <td>@Model.childActiveMap["7"]</td>
                                    <td>@Model.childActiveMap["8"]</td>
                                    <td>@Model.childActiveMap["9"]</td>
                                    <td>@Model.childActiveMap["10"]</td>
                                    <td>@Model.childActiveMap["11"]</td>
                                    <td>@Model.childActiveMap["12"]</td>
                                </tr>

                                <tr style="background-color:beige">
                                    <td>Total Registerer :</td>
                                    <td>@Model.childMap["1"]</td>
                                    <td>@Model.childMap["2"]</td>
                                    <td>@Model.childMap["3"]</td>
                                    <td>@Model.childMap["4"]</td>
                                    <td>@Model.childMap["5"]</td>
                                    <td>@Model.childMap["6"]</td>
                                    <td>@Model.childMap["7"]</td>
                                    <td>@Model.childMap["8"]</td>
                                    <td>@Model.childMap["9"]</td>
                                    <td>@Model.childMap["10"]</td>
                                    <td>@Model.childMap["11"]</td>
                                    <td>@Model.childMap["12"]</td>
                                </tr>



                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
            <!-- END SAMPLE TABLE PORTLET-->

        </div>
    </div>

    <div class="row" >
        <div class="col-md-12">
            <!-- BEGIN SAMPLE TABLE PORTLET-->
            <div class="portlet box red">
                <div class="portlet-title">
                    <div class="caption">
                        <i class="fa fa-cogs"></i>Transaksi
                    </div>
                    <div class="tools">
                        <a href="javascript:;" class="collapse">
                        </a>
                        <a href="#portlet-config" data-toggle="modal" class="config">
                        </a>
                        <a href="javascript:;" class="reload">
                        </a>
                        <a href="javascript:;" class="remove">
                        </a>
                    </div>
                </div>
                <div class="portlet-body">
                    <div class="table-scrollable">
                        <table class="table table-hover">
                            <thead>
                                <tr>
                                    <th>
                                        Nama
                                    </th>
                                    <th>
                                        Title
                                    </th>
                                    <th>
                                        Content
                                    </th>
                                    <th>
                                        Date
                                    </th>
                                </tr>
                            </thead>
                            <tbody>


                                @foreach (var item in Model.allTransaction)
                                {
                                <tr>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.CustomerID)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.VendorID)
                                    </td>
                                    <td>
                                       @Html.DisplayFor(modelItem => item.CreateBy)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.LastUpdatedBy)
                                    </td>
                                </tr>
                                }



                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
            <!-- END SAMPLE TABLE PORTLET-->

        </div>
    </div>
    <div class="row">
        <div class="col-md-6">
            <a href="/Manage/Index" class="btn blue">
                Back to My Account
            </a>
        </div>
    </div>
</div>
